{
  "name": "@nordicsemiconductor/rsrp-bar",
  "version": "0.0.0-development",
  "description": "React component to render an RSRP bar or a failover icon in case the reported value is not valid",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.tsx src/**",
    "start": "webpack serve",
    "test": "jest",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://nordicsemiconductor.github.io/asset-tracker-cloud-docs/",
  "keywords": [
    "Cloud",
    "IoT",
    "nRF Asset Tracker",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/rsrp-bar-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/rsrp-bar-js/issues"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "10.0.4",
    "@svgr/webpack": "6.1.2",
    "@types/jest": "27.4.0",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "eslint-plugin-react": "7.28.0",
    "jest": "27.4.5",
    "source-map-loader": "3.0.0",
    "ts-jest": "27.1.2",
    "ts-loader": "9.2.6",
    "webpack": "5.65.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "^4.7.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "files": [
    "README.md",
    "dist",
    "package-lock.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": ".+\\.spec\\.ts$"
  }
}
