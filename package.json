{
  "name": "@nordicsemiconductor/rsrp-bar",
  "version": "0.0.0-development",
  "description": "React component to render an RSRP bar or a failover icon in case the reported value is not valid",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.tsx src/**",
    "start": "webpack serve",
    "test": "jest",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://nordicsemiconductor.github.io/asset-tracker-cloud-docs/",
  "keywords": [
    "Cloud",
    "IoT",
    "Asset Tracker Cloud Example",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/rsrp-bar-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/rsrp-bar-js/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.0.2",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "7.0.2",
    "@svgr/webpack": "5.5.0",
    "@types/jest": "26.0.20",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "eslint": "7.19.0",
    "eslint-plugin-react": "7.22.0",
    "husky": "5.0.9",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "pinst": "2.1.4",
    "prettier": "2.2.1",
    "source-map-loader": "2.0.1",
    "ts-jest": "26.5.1",
    "ts-loader": "8.0.17",
    "typescript": "4.1.5",
    "webpack": "5.21.2",
    "webpack-cli": "4.5.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "files": [
    "README.md",
    "dist",
    "package-lock.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "jest": {
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      ".+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".+\\.spec\\.ts$",
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  }
}
